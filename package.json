{
  "name": "free-accessibility-menu",
  "version": "1.1.3",
  "description": "A free, client-side accessibility menu widget with zero dependencies",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "unpkg": "./dist/index.umd.min.js",
  "jsdelivr": "./dist/index.umd.min.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./css": "./dist/a11y-widget.css",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run test && npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@rollup/plugin-terser": "^0.4.4",
    "axe-core": "^4.8.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.0"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "widget",
    "wcag",
    "aria",
    "menu",
    "client-side",
    "zero-dependencies",
    "web-accessibility"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pachpul12/free-accessibility-menu.git"
  },
  "homepage": "https://github.com/pachpul12/free-accessibility-menu#readme",
  "bugs": {
    "url": "https://github.com/pachpul12/free-accessibility-menu/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
